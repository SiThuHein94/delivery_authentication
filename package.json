{
  "name": "delivery_authentication",
  "version": "1.0.0",
  "description": "authentication & authorization for customer, employee and admin",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test  jest --setupFiles=dotenv/config  ",
    "pretest": "cross-env NODE_ENV=test npm run db:reset",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-mig migration:make -n create-db-testing && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all ",
    "db:create:test": "cross-env NODE_ENV=test npx sequelize db:create",
    "start:dev": "nodemon -r dotenv/config index.js",
    "start": "node -r dotenv/config index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jest": "^29.4.1",
    "joi": "^17.7.0",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^3.1.0",
    "nodemon": "^2.0.20",
    "sequelize": "^6.28.0",
    "supertest": "^6.3.3",
    "winston": "^3.8.2"
  }
}
